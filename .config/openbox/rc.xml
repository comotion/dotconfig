        <action name="Move"/>
      </mousebind>
      <mousebind button="A-Middle" action="Drag">
        <action name="Resize"/>
      </mousebind>
      <mousebind button="A-Right" action="Press">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind button="A-Right" action="Drag">
        <action name="Resize"/>
